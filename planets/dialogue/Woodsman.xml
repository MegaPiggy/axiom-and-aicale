<DialogueTree xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Outer-Wilds-New-Horizons/new-horizons/main/NewHorizons/Schemas/dialogue_schema.xsd">
  <NameField>Woodsman</NameField>

  <DialogueNode>
    <Name>FIRST</Name>
    <EntryCondition>DEFAULT</EntryCondition>
    <SetCondition>TalkedToWoodsman</SetCondition>
    <Dialogue>
      <Page>Hello there, stranger.</Page>
    </Dialogue>
    <DialogueOptionsList>
      <DialogueOption>
        <Text>What instrument is that?</Text>
        <DialogueTarget>INSTRUMENT</DialogueTarget>
      </DialogueOption>
      <DialogueOption>
        <Text>What is your job about?</Text>
        <DialogueTarget>JOB</DialogueTarget>
      </DialogueOption>
      <DialogueOption>
        <Text>What are you doing?</Text>
        <DialogueTarget>DOING</DialogueTarget>
      </DialogueOption>
      <DialogueOption>
        <Text>What is this place?</Text>
        <DialogueTarget>PLACE</DialogueTarget>
      </DialogueOption>
      <DialogueOption>
        <Text>Why do you live here?</Text>
        <RequiredPersistentCondition>AxiomTravelerLive</RequiredPersistentCondition>
        <DialogueTarget>WHY</DialogueTarget>
      </DialogueOption>
      <DialogueOption>
        <Text>I have to go.</Text>
        <DialogueTarget>BYE</DialogueTarget>
      </DialogueOption>
    </DialogueOptionsList>
    <RevealFacts>
      <FactID>AXIOM_TRAVELER_BASE</FactID>
    </RevealFacts>
  </DialogueNode>

  <DialogueNode>
    <Name>MAIN</Name>
    <EntryCondition>TalkedToWoodsman</EntryCondition>
    <Dialogue>
      <Page>What else can I do for ya?.</Page>
    </Dialogue>
    <DialogueOptionsList>
      <DialogueOption>
        <Text>What instrument is that?</Text>
        <DialogueTarget>INSTRUMENT</DialogueTarget>
      </DialogueOption>
      <DialogueOption>
        <Text>What is your job about?</Text>
        <DialogueTarget>JOB</DialogueTarget>
      </DialogueOption>
      <DialogueOption>
        <Text>What are you doing?</Text>
        <DialogueTarget>DOING</DialogueTarget>
      </DialogueOption>
      <DialogueOption>
        <Text>What is this place?</Text>
        <DialogueTarget>PLACE</DialogueTarget>
      </DialogueOption>
      <DialogueOption>
        <Text>Why do you live here?</Text>
        <RequiredPersistentCondition>AxiomTravelerLive</RequiredPersistentCondition>
        <DialogueTarget>WHY</DialogueTarget>
      </DialogueOption>
      <DialogueOption>
        <Text>I have to go.</Text>
        <DialogueTarget>BYE</DialogueTarget>
      </DialogueOption>
    </DialogueOptionsList>
  </DialogueNode>

  <DialogueNode>
    <Name>DOING</Name>
    <Dialogue>
      <Page>Just playing some music by the campfire.</Page>
    </Dialogue>
    <DialogueOptionsList>
      <DialogueOption>
        <Text>I see</Text>
        <DialogueTarget>MAIN</DialogueTarget>
      </DialogueOption>
    </DialogueOptionsList>
  </DialogueNode>

  <DialogueNode>
    <Name>PLACE</Name>
    <SetPersistentCondition>AxiomTravelerLive</SetPersistentCondition>
    <Dialogue>
      <Page>This is the traveler's treehouse.</Page>
      <Page>All 4 of us live here at this campsite.</Page>
    </Dialogue>
    <DialogueOptionsList>
      <DialogueOption>
        <Text>Why do you live here?</Text>
        <DialogueTarget>WHY</DialogueTarget>
      </DialogueOption>
      <DialogueOption>
        <Text>Awesome</Text>
        <DialogueTarget>MAIN</DialogueTarget>
      </DialogueOption>
    </DialogueOptionsList>
  </DialogueNode>

  <DialogueNode>
    <Name>WHY</Name>
    <Dialogue>
      <Page>This is our base. We stop here and rest after astronauting.</Page>
      <Page>There is much to explore out there, but it can wait for sleep.</Page>
    </Dialogue>
    <DialogueOptionsList>
      <DialogueOption>
        <Text>So true</Text>
        <DialogueTarget>MAIN</DialogueTarget>
      </DialogueOption>
    </DialogueOptionsList>
  </DialogueNode>

  <DialogueNode>
    <Name>INSTRUMENT</Name>
    <Dialogue>
      <Page>This is my harmonica. The perfect instrument for someone like me!</Page>
    </Dialogue>
    <DialogueOptionsList>
      <DialogueOption>
        <Text>I agree</Text>
        <DialogueTarget>MAIN</DialogueTarget>
      </DialogueOption>
    </DialogueOptionsList>
  </DialogueNode>

  <DialogueNode>
    <Name>JOB</Name>
    <Dialogue>
      <Page>My job is to go in and scout out the area first before the others come.</Page>
      <Page>The only thing dangerous I've encountered so far is those Anglerfish back in our old solar system.</Page>
    </Dialogue>
    <DialogueTarget>MAIN</DialogueTarget>
  </DialogueNode>

  <DialogueNode>
    <Name>BYE</Name>
    <Dialogue>
      <Page>Bye bye.</Page>
    </Dialogue>
  </DialogueNode>

</DialogueTree>